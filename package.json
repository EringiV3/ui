{
  "name": "eringiv3-ui",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "check": "run-p format-check lint typecheck",
    "format": "prettier --write .",
    "format-check": "prettier --check .",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx,.json src",
    "typecheck": "tsc",
    "prepare": "husky install",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "lint-staged": {
    "*": "prettier --write .",
    "*.@(ts|tsx)": "tsc",
    "*.@(js|ts|tsx)": "eslint --ext .js,.jsx,.ts,.tsx,.json src"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ress": "^5.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@storybook/addon-actions": "^7.0.0-rc.5",
    "@storybook/addon-essentials": "^7.0.0-rc.5",
    "@storybook/addon-interactions": "^7.0.0-rc.5",
    "@storybook/addon-links": "^7.0.0-rc.5",
    "@storybook/addon-mdx-gfm": "^7.0.0-rc.5",
    "@storybook/react": "^7.0.0-rc.5",
    "@storybook/react-vite": "^7.0.0-rc.5",
    "@storybook/testing-library": "^0.0.14-next.1",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@tsconfig/strictest": "^1.0.2",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "babel-loader": "^8.3.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-storybook": "^0.6.11",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.5",
    "storybook": "^7.0.0-rc.5",
    "typescript": "^4.9.5",
    "vite": "^4.2.0",
    "vite-tsconfig-paths": "^4.0.7"
  }
}
